@{
}
@model List<Object>
    <h2>
        Liệt kê các Supplier (CompanyName, ContactName) cung cấp 10 Product trở lên (ProductName, UnitPrice, UnitsInStock).
    </h2>
<table border="">
    <tr>
        <th>ContactName</th>
        <th>CompanyName</th>
    </tr>
 
    @foreach (var item in Model)
    {
<tr>
    <td>@item.GetType().GetProperty("ContactName").GetValue(item, null)</td>
    <td>@item.GetType().GetProperty("CompanyName").GetValue(item, null)</td>
</tr>

    }
</table>