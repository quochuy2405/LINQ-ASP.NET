@{
}
@model List<Object>
    <h2>Đếm số Product của từng Category (CatagoryName, QuantityPro)</h2>
<table border="">
    <tr>

    <th>CategoryName</th>
    <th>QuantityPro</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.GetType().GetProperty("CategoryName").GetValue(item, null)</td>
            <td>@item.GetType().GetProperty("QuantityPro").GetValue(item, null)</td>

        </tr>

    }
</table>