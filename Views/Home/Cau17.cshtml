@{
}
@model List<Object>
<h2>Đếm số lần mua hàng của Customer (ContactName, Quantity)</h2>
<table border="">
    <tr>

        <th>ContactName</th>
        <th>QuantityPro</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.GetType().GetProperty("ContactName").GetValue(item, null)</td>
            <td>@item.GetType().GetProperty("Quantity").GetValue(item, null)</td>

        </tr>

    }
</table>