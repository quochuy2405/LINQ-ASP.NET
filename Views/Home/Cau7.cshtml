@{
}
@model List<Object>
    <h2>Liệt kê tất cả các Order (OrderDate, ShipName) được ship đến ShipCountry là “Germany”</h2>
<table border="">
    <tr>
        <th>OrderDate</th>
        <th>ShipName</th>
    </tr>
   

    @foreach (var item in Model)
    {
<tr>
    <td>@item.GetType().GetProperty("OrderDate").GetValue(item, null)</td>
    <td>@item.GetType().GetProperty("ShipName").GetValue(item, null)</td>
</tr>

    }
</table>