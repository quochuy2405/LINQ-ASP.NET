@{
}
@model List<Object>
    <h2>Liệt kê các Product (ProductName) có ProductName bắt đầu bằng chữ “G”</h2>
    <table border="">
        <tr>

        <th>ProductName</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.GetType().GetProperty("ProductName").GetValue(item, null)</td>
            </tr>

        }
    </table>